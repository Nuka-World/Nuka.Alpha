{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "NoAuth": "true",
  "URLS": {
    "IdentityApiUrl": "https://localhost:5000"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(LocalDb)\\MSSQLLocalDB;database=Nuka.Sample.API;trusted_connection=yes;"
  },
  "ServiceBusEnabled": "false",
  "ServiceBusConfig": {
    "ConnectionString": "Endpoint=sb://x.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=xxx",
    "TopicName": "demotopic",
    "SubscriptionName": "demosub"
  }
}